<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="../global.css" />
    <link rel="stylesheet" href="style.css" />
  </head>
  <body>
    <div class="desktop">
      <div class="overlap-wrapper">
        <div class="overlap">
          <div class="group">
            <div class="overlap-group">
              <div class="overlap-group-wrapper">
                <div class="div"><div class="text-wrapper">Concepts</div></div>
              </div>
              <div class="div-wrapper">
                <div class="div"><div class="text-wrapper">Style</div></div>
              </div>
              <div class="group-2">
                <div class="overlap-2"><div class="text-wrapper">Mechanics</div></div>
                <div class="group-3">
                  <div class="div">
                    <img class="rectangle" src="img/rectangle-13.svg" />
                    <img class="rectangle" src="img/rectangle-13.svg" />
                    <div class="text-wrapper">Abilities</div>
                  </div>
                </div>
              </div>
              <div class="text-wrapper-2">Game Generation Inputs</div>
            </div>
            <div class="top-bar"></div>
          </div>
          <p class="application-title">Online Trading Card Game Maker</p>
          <img class="play-button" src="img/rectangle-6.png" />
          <img class="trade-button" src="img/rectangle-6.png" />
          <img class="make-button" src="img/rectangle-5.png" />
          <img class="collect-button" src="img/rectangle-6.png" />
          <div class="play-button-text">Play</div>
          <div class="trade-button-text">Trade</div>
          <div class="make-button-text">Make</div>
          <div class="collect-button-text">Collect</div>
          <img class="user-logo" src="img/group-2.png" />
          <img class="group-5" src="img/group-10.png" />
          <img class="group-6" src="img/group-6.png" />
          <div class="group-wrapper">
            <div class="group-7">
              <div class="group-8">
                <div class="overlap-group-2"><div class="text-wrapper-7">Deck Name</div></div>
              </div>
            </div>
          </div>
          <div class="group-9">
            <div class="group-7">
              <div class="group-8">
                <div class="overlap-group-2"><div class="text-wrapper-8">View Current Rule Iteration</div></div>
              </div>
            </div>
          </div>
          <div class="group-10">
            <div class="group-7">
              <div class="group-8">
                <div class="overlap-group-2"><p class="text-wrapper-9">Browse Cards in Current Deck</p></div>
              </div>
            </div>
          </div>
          <div class="group-11">
            <div class="group-7">
              <div class="group-12">
                <div class="overlap-group-3"><div class="text-wrapper-10">Help</div></div>
              </div>
            </div>
          </div>
          <img class="group-13" src="img/group-4.png" />
          <img class="group-14" src="img/group-12.png" />
          <img class="group-15" src="img/group-13.png" />
          <img class="group-16" src="img/group-11.png" />
          <img class="group-17" src="img/group-5.png" />
          <img class="group-18" src="img/group-14.png" />
          <img class="group-19" src="img/group-15.png" />
          <img class="group-20" src="img/group-3.png" />
          <p class="text-wrapper-11">Card No. X out of Y</p>
          <div class="text-wrapper-12">Card Name</div>
        </div>
      </div>
    </div>
  </body>
</html>
