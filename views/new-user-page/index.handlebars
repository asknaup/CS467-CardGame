<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8" />
  <link rel="stylesheet" href="globals.css" />
  <link rel="stylesheet" href="style.css" />
</head>

<body>
  <div class="new-user-page">
    <div class="overlap-wrapper">
      <div class="overlap">
        <div class="group">
          <div class="rectangle"></div>
          <div class="div"></div>
        </div>
        <p class="text-wrapper">Online Trading Card Game Maker</p>
        <img class="img" src="img/group-2.png" />
        <div class="rectangle-2"></div>
        <form class="text-font" id="addUser">
          <div class="text-wrapper-2">Enter Username</div>
          <div class="text-wrapper-3">Enter Password</div>
          <div class="text-wrapper-4">Repeat Password</div>
          <div class="text-wrapper-5">Enter Email</div>
          <div class="group-2">
            <div class="rectangle-3"></div>
            <div class="rectangle-4"></div>
          </div>
          <div class="text-wrapper-6">Welcome New User!</div>
          
          <div class="overlap-group-wrapper">
              <input class="btn" type="submit" value="SUBMIT">
          </div>
          {{!-- <div class="overlap-group">
            <div class="text-wrapper-7">SUBMIT</div>
          </div> --}}
          {{!-- </input> --}}
          <div class="group-8">
            <input class="rectangle-3" id="usn" type="text" placeholder="Username" required></input>
            <input class="rectangle-5" id="pwd" type="password" placeholder="Password" required></input>
          </div>
        </form>
        <script src="/js/addUser.js"></script>
        <img class="information-icon" src="img/information-icon-must-have-iconset-visualpharm-26-1.png" />
        <div class="div-wrapper">
          <div class="overlap-2">
            <div class="group-3"></div>
            <div class="group-4">
              <div class="group-5">
                <div class="overlap-group-2">
                  <div class="text-wrapper-8">Make</div>
                </div>
              </div>
              <div class="text-wrapper-9">Collect</div>
              <div class="group-wrapper">
                <div class="group-6">
                  <div class="overlap-group-2">
                    <div class="text-wrapper-10">Trade</div>
                  </div>
                </div>
              </div>
              <div class="group-7">
                <div class="group-6">
                  <div class="overlap-group-2">
                    <div class="text-wrapper-11">Play</div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</body>

</html>