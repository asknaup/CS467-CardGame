<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="../global.css" />
    <link rel="stylesheet" href="style.css" />
  </head>
  <body>
    <div class="desktop">
      <div class="overlap-wrapper">
        <div class="overlap">
          <div class="group">
            <div class="overlap-group"></div>
            <div class="top-bar"></div>
          </div>
          <p class="application-title">Online Trading Card Game Maker</p>
          <img class="play-button" src="img/rectangle-6.png" />
          <img class="trade-button" src="img/rectangle-6.png" />
          <img class="make-button" src="img/rectangle-5.png" />
          <img class="collect-button" src="img/rectangle-6.png" />
          <div class="play-button-text">Play</div>
          <div class="trade-button-text">Trade</div>
          <div class="make-button-text">Make</div>
          <div class="collect-button-text">Collect</div>
          <img class="user-logo" src="img/group-2.png" />
        </div> <!-- class="overlap" -->
      </div> <!-- class="overlap-wrapper" -->
    </div> <!-- class="desktop" -->

    <div class="reset-password-page">
      <div class="overlap-wrapper-a">
        <div class="overlap-a">

          <div class="rectangle-b"></div>
          <div class="text-wrapper-b">Enter Username</div>
          <div class="text-wrapper-c">Enter New Password</div>
          <div class="text-wrapper-d">Repeat New Password</div>
          <div class="text-wrapper-e">Enter Email</div>
          <div class="group-b">
            <div class="rectangle-c"></div>
            <div class="rectangle-d"></div>
          </div>
          <div class="group-c">
            <div class="rectangle-c"></div>
            <div class="rectangle-e"></div>
          </div>
          <div class="text-wrapper-f">Reset Password</div>
          <div class="overlap-group-wrapper-a">
            <div class="overlap-group-a"><div class="text-wrapper-g">SUBMIT</div></div>
          </div>
          <img class="information-icon" src="img/information-icon-must-have-iconset-visualpharm-26-1.png" />
  
        </div> <!-- class="overlap-a" -->
      </div> <!-- class="overlap-wrapper-a" -->
    </div> <!-- class="reset-password-page -->
  </body>
</html>
